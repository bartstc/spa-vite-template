{
  "name": "spa-vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "preview": "vite preview",
    "lint": "CI=true eslint --ignore-pattern '*.d.ts' --ignore-pattern '*.test.*' --ext ts,tsx --max-warnings 0 src",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "storybook-latest-node": "export NODE_OPTIONS=--openssl-legacy-provider; start-storybook -p 6006",
    "test-storybook": "test-storybook --watch",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/jest": "^0.0.10",
    "@storybook/react": "^6.5.14",
    "@storybook/test-runner": "^0.9.1",
    "@storybook/testing-library": "^0.0.13",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.27.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-storybook": "^0.6.8",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "storybook-dark-mode": "^2.0.4",
    "typescript": "^4.9.3",
    "vite": "^3.2.3",
    "vite-plugin-checker": "^0.5.1",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.25.2"
  }
}
